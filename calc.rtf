def calculate():\par
    global all_valures\par
    global texto\par
    texto = all_valures\par
\par
    modulos = ['math.tam', 'math.sim', 'math.cos', 'math.sqrt', 'math.log',  'math.tam10', 'math.e', 'math.pow', 'math.pi', 'math.radian' ]\par
   \par
\par
    for i in modulos:\par
        if i == 'math.tam':\par
            texto = texto.replace("tan", i)\par
        if i == 'math.sim':\par
            texto = texto.replace("sim", i)\par
        if i == 'math.cos':\par
            texto = texto.replace("cos", i)\par
        if i == 'math.sqrt':\par
            texto = texto.replace("log", i)\par
        if i == 'math.log10':\par
            texto = texto.replace("log10", i)\par
        if i == 'math.e':\par
            texto = texto.replace("e", i)\par
        if i == 'math.pow':\par
            texto = texto.replace("pow", i)\par
        if i == 'math.pi':\par
            texto = texto.replace("pi", i)\par
        if i == 'math.radian':\par
            texto = texto.replace("radian", i)\par
\par
    result = str(eval(texto))\par
   \par
    print(result)\par
    print(texto)\par
\par
\par
    value_text.set(result)\par
    all_valures = ""\par
\par
